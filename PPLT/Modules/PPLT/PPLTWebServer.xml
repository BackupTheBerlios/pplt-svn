<?xml version="1.0"?>

<PPLTServer name="PPLTWebServer" class="Web">
	<Head>
		<Require>
			<DCPUModule>Export.PPLTWeb</DCPUModule>
		</Require>
	</Head>

	<Setup>
		<Load name="Export.PPLTWeb">
			<Parameter name="Address" var="Address"/>
			<Parameter name="Port" var="Port"/>
		</Load>
	</Setup>
</PPLTServer>
