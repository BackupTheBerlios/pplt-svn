OBJS    = HexDumpModule.o LoopbackModule.o NULLModule.o RandomModule.o TimeModule.o HexifierModule.o
LDFLAGS = @LDFLAGS@ -shared
LIBS    = @LIBS@
CXX     = @CXX@

all: $(OBJS)
	$(CXX) $(LIBS) $(LDFLAGS) -o ppltstdmodules.so $(OBJS)

clean:
	rm -f *.o
	rm -f *.so


distclean: clean
	rm -f configure Makefile config.status config.h
	rm -rf autom4te.cache
	rm -f config.log


install:

