OBJS    = HexDumpModule.o LoopbackModule.o NULLModule.o RandomModule.o TimeModule.o
LDFLAGS = @LDFLAGS@ -shared
LIBS    = @LIBS@


all: $(OBJS)
	$(CC) $(LIBS) $(LDFLAGS) -o ppltstdmodules.so $(OBJS)

clean:
	rm -f *.o
	rm -f *.so


dist-clean: clean
	rm -f configure Makefile config.status config.h
	rm -rf autom4te.cache
	rm -f config.log

